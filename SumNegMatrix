#include <stdio.h>

int main() {
    // Define a 2D array
    int array[3][3] = {
        {1, -2, 3},
        {-4, 5, -6},
        {7, -8, 9}
    };

    // Variables to store sum of negative numbers
    int sum = 0;

    // Iterate through each element of the array
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            // Check if the element is negative
            if (array[i][j] < 0) {
                // Add the negative number to the sum
                sum += array[i][j];
            }
        }
    }

    // Print the sum of negative numbers
    printf("Sum of negative numbers: %d\n", sum);

    return 0;
}
